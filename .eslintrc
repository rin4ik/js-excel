{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        // Indent with 4 spaces
        "indent": ["error", 4], 
        "semi": "off",
        "arrow-parens": "off",
        "object-curly-spacing": "off",
        "comma-dangle": "off",
        "require-jsdoc": "off"
    },
    "extends": ["eslint:recommended", "google"]
}
